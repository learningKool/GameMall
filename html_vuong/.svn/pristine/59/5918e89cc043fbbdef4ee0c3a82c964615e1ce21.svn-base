
.admin{
	width:768px;display:block;position: relative;
	margin: 5% auto;
}

.line {
	background-image: -webkit-gradient(linear, 0% 50%, 100% 50%, color-stop(0%, #1B1B24), color-stop(10%, #2F2F3E), color-stop(90%, #2F2F3E), color-stop(100%, #1B1B24));
	background-image: -webkit-linear-gradient(left, #1B1B24,#2F2F3E 10%,#2F2F3E 90%,#1B1B24);
	background-image: -moz-linear-gradient(left, #1B1B24,#2F2F3E 10%,#2F2F3E 90%,#1B1B24);
	background-image: -o-linear-gradient(left, #1B1B24,#2F2F3E 10%,#2F2F3E 90%,#1B1B24);
	background-image: linear-gradient(left, #1B1B24,#2F2F3E 10%,#2F2F3E 90%,#1B1B24);
	background-position: bottom center;
	background-repeat: no-repeat;
	background-size: 100% 1px;
	padding-bottom: 10px;
}
.login-form{
	margin: 10% auto;
}
.button { padding-top: 10px;}

.list-account .header{height: 30px; vertical-align: baseline;font-size: 11px; border-bottom:1px solid #8C6239;font-weight:bold}
.td.account{width:30%; padding-left: 5%;}
.td.date-regis{width:40%;}
.td.btn-active{width:30%;float:right;}



